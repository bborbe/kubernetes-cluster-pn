{
	"kuberntes-version": "v1.8.1",
	"etcd-version": 3,
	"region": "sun",
	"update-reboot-strategy": "reboot",
	"features": {
		"kvm": true
	},
	"hosts": [
		{
			"name": "sun.pn.benjamin-borbe.de",
			"lvm-vg": "system",
			"vm-prefix": "",
			"disk-prefix": "",
			"kubernetes-device": "host-k8s",
			"kubernetes-network": "172.16.72.0/24",
			"kubernetes-dns": "8.8.8.8",
			"nodes": [
				{
					"name": "kubernetes",
					"etcd": true,
					"worker": true,
					"master": true,
					"nfsd": true,
					"nfssize": "5G",
					"rootsize": "9.5G",
					"dockersize": "9.5G",
					"kubeletsize": "9.5G",
					"cores": 4,
					"memory": 10000,
					"number": 1
				}
			]
		}
	]
}
